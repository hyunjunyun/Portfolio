package dbcal;

import java.awt.BorderLayout;
import java.awt.FlowLayout;

import javax.swing.JFrame;
import javax.swing.JPanel;

class Dbcal extends JFrame {
	public static void main(String[] args) {

	}

	public Dbcal(){
		super("STD record Calculator");
		getContentPane().setLayout(new BorderLayout(1,1));
		setSize(520,440);
		setVisible(true);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE); 	
		JPanel npl = new JPanel();
		getContentPane().add(npl);
		npl.setLayout(null);
		
		
		JPanel tpl = new JPanel();
		tpl.setLocation(5, 5);
//		tpl.setLayout();
		npl.add(tpl);
		
		JPanel ipl = new JPanel();
		ipl.setLocation(20, 10);
//		ipl.setLayout();
		npl.add(ipl);
		JPanel bpl = new JPanel();
		bpl.setLocation(25, 10);
		bpl.setLayout(new FlowLayout(10,220,230));
		npl.add(bpl);
//		npl.add(ipl,BorderLayout.WEST);
//		npl.add(tpl, BorderLayout.EAST);
//		npl.add(bpl, BorderLayout.EAST);
//		
		tpl.setSize(10,10);
		
		
		ipl.setLayout(new BorderLayout());
		ipl.setSize(0,0);
		
		
		bpl.setSize(0,0);
		bpl.setLayout(new BorderLayout());
	}
}